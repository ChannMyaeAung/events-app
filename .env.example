# Copy this to .env or into backend/.env for local development
# Replace values as needed for the environment
# Postgres connection used by the backend
DATABASE_URL=postgres://postgres:postgres@localhost:5433/events?sslmode=disable

# Backend server port
PORT=8080

# JWT secret used by the backend for auth (replace before production)
JWT_SECRET=replace-me-with-a-secure-secret

# Migrations dir (backend reads this env var)
MIGRATIONS_DIR=/migrations
